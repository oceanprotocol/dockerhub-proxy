    "@vercel/node": "^1.11.0",
    "eslint": "^7.27.0",
    "eslint-config-oceanprotocol": "^1.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.0",
    "typescript": "^4.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oceanprotocol/dockerhub-proxy"
