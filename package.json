{
  "name": "@oceanprotocol/dockerhub-proxy",
  "description": "Microservice API endpoint to validate Docker image URLs.",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "homepage": "https://oceanprotocol.com",
  "scripts": {
    "start": "vercel dev",
